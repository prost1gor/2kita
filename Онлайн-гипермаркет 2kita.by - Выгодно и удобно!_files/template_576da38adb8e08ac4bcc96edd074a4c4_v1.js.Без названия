; /* Start:"a:4:{s:4:"full";s:52:"/local/components/sh/favorite/init.js?15513476131849";s:6:"source";s:37:"/local/components/sh/favorite/init.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/



function FavoriteEventInit(event, bxobj) {


    $(".js_favorite_elem", bxobj).off("click.favoriteAddChange").on("click.favoriteAddChange", function () {
        var
                infavor = $(this).is(".js_favor"),
                productID = $(this).data("productid");

        var urlQuery = [];

        if (!infavor) {
            $(this).addClass("js_favor active");
            $(this).closest('.js-favorite-heart').addClass('active');
            urlQuery["FAVORITE_ADD"] = productID;
        } else {
            $(this).removeClass("js_favor active");
            $(this).closest('.js-favorite-heart').removeClass('active');
            urlQuery["FAVORITE_DEL"] = productID;
        }
        submitFavorite(urlQuery)
    });



    function submitFavorite(urlQuery) {

        var GetUrl = urlQuery,
                dataSet = false;

        $(".js_set_favorite_values", $("#favoriteSubmitAction")).each(function () {
            var name = $(this).attr("name");
            if (GetUrl[name]) {
                $(this).val(GetUrl[name]);
                dataSet = true;
            }
        });


        if (dataSet) {
            $("#favoriteSubmitAction").triggerHandler("submit");
        }

    }


}

function FavoriteResult( event, bxobj ){
    
    if( $('.js_total_favorite', bxobj).length ){
        var result = parseInt( $('.js_total_favorite', bxobj).val() );
        var count = $( '.js_total_favorite_count' );
        count.html( result );
    }

}

$("document").ready(function () {
    $("body").on("onAjaxReload.FavoriteEventInit", FavoriteEventInit).triggerHandler("onAjaxReload.FavoriteEventInit", [$("body")]);
    $("body").on("onAjaxReload.FavoriteResult", FavoriteResult).triggerHandler("onAjaxReload.FavoriteResult", [$("body")]);

});




/* End */
;
; /* Start:"a:4:{s:4:"full";s:51:"/local/components/sh/compare/init.js?15513581242680";s:6:"source";s:36:"/local/components/sh/compare/init.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/



function CompareEventInit(event, bxobj) {


    $(".js-comparelist-informer", bxobj).off("click.compareCloseChange").on("click.compareCloseChange", function () {
        $('.js-comparelist').addClass('hidden');
    });


    $(".js_compare_elem", bxobj).off("click.compareAddChange").on("click.compareAddChange", function () {
        var
                incomp = $(this).is(".js_comp"),
                productID = $(this).data("productid"),
                productName = $(this).data("productname");

        var urlQuery = [];

        if (!incomp) {
            $(this).addClass("js_comp active");
            $(this).closest('.js-compare-heart').addClass('active');
            urlQuery["COMPARE_ADD"] = productID;
            urlQuery["COMPARE_NAME_ITEMS"] = productName;
        } else {
            $(this).removeClass("js_comp active");
            $(this).closest('.js-compare-heart').removeClass('active');
            urlQuery["COMPARE_DEL"] = productID;
            urlQuery["COMPARE_NAME_ITEMS"] = productName;
        }
        submitCompare(urlQuery)
    });



    function submitCompare(urlQuery) {

        var GetUrl = urlQuery,
                dataSet = false;

        $(".js_set_compare_values", $("#compareSubmitAction")).each(function () {
            var name = $(this).attr("name");
            if (GetUrl[name]) {
                $(this).val(GetUrl[name]);
                dataSet = true;
            }
        });


        if (dataSet) {
            $("#compareSubmitAction").triggerHandler("submit");
        }

    }


}

function declOfNum(number, titles) {
    cases = [2, 0, 1, 1, 1, 2];
    return titles[ (number%100>4 && number%100<20)? 2 : cases[(number%10<5)?number%10:5] ];
}

function CompareResult( event, bxobj ){
    
    if( $('.js_total_compare', bxobj).length ){
        var result = parseInt( $('.js_total_compare', bxobj).val() );
        var count = $( '.js_total_compare_count' );
        count.html( result );

        var count = $( '.js_total_compare_count_text' );
        count.html( result + ' ' + declOfNum(result, ['', '', '']) );

        if(result > 0)
            $('.js-comparising-list').removeClass('hidden');
        else
            $('.js-comparising-list').addClass('hidden');

    }

}

$("document").ready(function () {
    $("body").on("onAjaxReload.CompareEventInit", CompareEventInit).triggerHandler("onAjaxReload.CompareEventInit", [$("body")]);
    $("body").on("onAjaxReload.CompareResult", CompareResult).triggerHandler("onAjaxReload.CompareResult", [$("body")]);

});




